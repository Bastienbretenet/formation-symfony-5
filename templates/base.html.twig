    {% include "shared/_navbar.html.twig" %}

    <body>
        <div class="pt-5">
            {% block body %}{% endblock %}
        </div>
    </body>
    {% block javascripts %}
        {{ encore_entry_script_tags('app') }}
    {% endblock %}
</html>
